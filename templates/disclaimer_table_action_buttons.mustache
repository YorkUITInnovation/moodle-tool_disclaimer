{{#showEditButtons}}
<a href="{{edit_url}}"
   class="btn btn-outline-primary btn-sm"
   title="{{#str}}edit, tool_disclaimer{{/str}}"><i class="fa fa-pencil"></i></a>
{{/showEditButtons}}
{{#showDelButtons}}
<button type="button"
        class="btn btn-outline-danger btn-sm btn-tool-disclaimer-delete-disclaime"
        title="{{#str}}delete, tool_disclaimer{{/str}}"
        data-id="{{id}}"
        data-name="{{name}}"><i class="fa fa-trash"></i></button>
{{/showDelButtons}}
